{
  "name": "googlemaps",
  "version": "2025.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production @servoy/googlemaps",
    "build_debug": "ng build @servoy/googlemaps --watch",
    "build_debug_nowatch": "ng build @servoy/googlemaps --watch=false",
    "make_release": "ng build --configuration production @servoy/googlemaps && node scripts/build.js",
    "test": "ng test --watch --browsers Chrome",
    "test_edge": "ng test --watch --browsers Edge",
    "test_edge_nowatch": "ng test --watch=false --browsers Edge",
    "test_headless": "ng test --browsers headlessChrome --watch=false",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run --config video=false --component --browser chrome",
    "cy:run_spec": "cypress run --config video=false --component --browser chrome --headed --no-exit --spec "
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.1.1",
    "@angular/compiler": "19.1.1",
    "@angular/core": "19.1.1",
    "@angular/forms": "19.1.1",
    "@googlemaps/markerclusterer": "2.5.3",
    "@servoy/public": "2025.3.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.1",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "19.1.1",
    "@angular/compiler-cli": "19.1.1",
    "@angular/platform-browser": "19.1.1",
    "@angular/platform-browser-dynamic": "19.1.1",
    "@chiragrupani/karma-chromium-edge-launcher": "2.4.1",
    "@types/google.maps": "3.58.1",
    "@types/jasmine": "5.1.5",
    "@types/luxon": "3.4.2",
    "@types/node": "^22.10.7",
    "@types/supercluster": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
